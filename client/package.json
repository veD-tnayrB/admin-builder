{
	"name": "@essential/builder",
	"version": "0.0.1",
	"title": "Essential builder",
	"description": "",
	"license": "ISC",
	"layout": "main-layout",
	"template": "template/template.json",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"params": {
		"application": {
			"name": "Essential builder"
		},
		"server": "http://localhost:5001"
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"environment": "development",
				"platform": "web",
				"ports": {
					"bundles": 6510
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/kernel": "~0.1.9",
		"@beyond-js/react-18-widgets": "~1.0.1",
		"@beyond-js/reactive": "^2.0.5",
		"@tippyjs/react": "^4.2.6",
		"pragmate-ui": "^1.0.0-beta.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"socket.io-client": "^4.5.4",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@beyond-js/local": "~0.1.3",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9"
	},
	"static": {
		"includes": [
			"assets"
		]
	}
